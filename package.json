{
  "name": "panelsnap",
  "version": "0.0.0-development",
  "description": "A JavaScript plugin that provides snapping functionality to a set of panels within your interface.",
  "main": "lib/panelsnap.cjs.js",
  "browser": "lib/panelsnap.umd.js",
  "module": "lib/panelsnap.esm.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/guidobouman/panelsnap"
  },
  "author": "Guido Bouman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guidobouman/panelsnap/issues"
  },
  "homepage": "https://panelsnap.com/",
  "keywords": [
    "snap",
    "snapping",
    "element",
    "elements",
    "panel",
    "panels",
    "scroll",
    "scrolling"
  ],
  "dependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "tweezer.js": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-jest": "^23.6.0",
    "commitizen": "^3.1.2",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.14.0",
    "jest": "^23.6.0",
    "rollup": "^0.68.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-license": "^0.7.0",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-uglify": "^6.0.2",
    "semantic-release": "^15.13.19"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "lint": "eslint .",
    "test": "jest --coverage",
    "semantic-release": "semantic-release",
    "prepack": "npm run build",
    "commit": "git-cz"
  },
  "browserslist": [
    "> 0.25%"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
